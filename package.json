{
  "name": "@coachly/ga4-admin-mcp",
  "version": "0.1.0",
  "description": "MCP server for Google Analytics 4 Admin API - automate GA4 configuration with natural language",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ga4-admin-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/integration/**/*.test.ts'",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-analytics",
    "ga4",
    "admin-api",
    "automation",
    "claude-code",
    "custom-dimensions",
    "conversion-events"
  ],
  "author": "Coachly Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_ORG/ga4-admin-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_ORG/ga4-admin-mcp/issues"
  },
  "homepage": "https://github.com/YOUR_ORG/ga4-admin-mcp#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "QUICK_START.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "google-auth-library": "^9.0.0",
    "googleapis": "^131.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
